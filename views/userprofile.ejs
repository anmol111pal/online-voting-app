        <script>   
            function vote(id) {
                console.log(id);

                if (id === "vote1") {
                    const x = Vote.findOne({ name: "BJP" }).voteCount;

                    Vote.updateOne({ name: "BJP" }, {
                        $set: {
                            voteCount: x + 1
                        }
                    });
                }

                else if (id === "vote2") {
                    const x = Vote.findOne({ name: "INC" }).voteCount;
                    Vote.updateOne({ name: "INC" }, {
